<launch>
  <arg name="camera_position" default="left"/>
  <include file="$(find jetson_nano_csi_cam)/launch/jetson_dual_csi_cam.launch" />
    <param name="approximate_sync" value="true"/>
    <param name="queue_size" value="100"/>
  <node pkg="stereo_image_proc" name="stereo_image_proc" type="stereo_image_proc" ns="stereo"/>
  <node pkg="jnmouse_ros_examples" name="set_image_points" type="set_image_points.py" output="screen" required="true"/>
    <param name="camera_position" value="$(arg camera_position)"/>
</launch>
